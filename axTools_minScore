func filterAxt(input string, output string, minScore int) {
	ioWriter := fileio.EasyCreate(output)
	data, _ := axt.GoReadToChan(input)

	var index int = 0
	for each := range data {
		if each.score >= score {
			axt.WriteToFileHandle(ioWriter, each, index)
			index++
		}
	}
}
